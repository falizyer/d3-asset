{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true
  },
  "globals": {
    "window": true
  },
  "plugins": [
  ],
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "quotes": [
      2,
      "double"
    ],
    "max-params": [
      2,
      3
    ]
  }
}